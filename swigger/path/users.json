{
    "get": {
        "summary": "Get list of users",
        "operationId": "getUsers",
        "responses": {
            "200": {
                "description": "A list of users",
                "content": {
                    "application/json": {
                        "schema": {
                            "type": "array",
                            "items": {
                                "type": "object",
                                "properties": {
                                    "id": {
                                        "type": "integer",
                                        "description": "User ID"
                                    },
                                    "name": {
                                        "type": "string",
                                        "description": "User Name"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}